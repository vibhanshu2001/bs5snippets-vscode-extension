{
    "name": "bs5snippets",
    "displayName": "bs5snippets",
    "description": "Advanced Bootstrap 5 snippets generator",
    "version": "2.1.2",
    "publisher": "VibhanshuGarg",
    "repository": {
        "type": "git",
        "url": "https://github.com/vibhanshu2001/bs5snippets-vscode-extension"
    },
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Snippets"
    ],
    "icon": "icon.jpg",
    "contributes": {
        "commands":[
            {
                "command": "!bs5",
                "title": "Bootstrap 5 boilerplate with cdn links"
            },
            {
                "command": "!bs5nav",
                "title": "Bootstrap 5 navbar"
            },
            {
                "command": "!bs5alert",
                "title": "Bootstrap 5 alert"
            }
        ],
        "snippets": [
            {
                "language": "html",
                "path": "./snippets/snippets.code-snippets"
            },
            {
                "language": "javascript",
                "path": "./snippets/snippets.code-snippets"
            },
            {
                "language": "javascriptreact",
                "path": "./snippets/snippets.code-snippets"
            },
            {
                "language": "php",
                "path": "./snippets/snippets.code-snippets"
            }
        ]
    }
}
